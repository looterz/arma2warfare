    class Handle_Purchase
    {
      name = "Handle_Purchase";
      init = /*%FSM<STATEINIT""">*/"format[""AITEAM.FSM: Team %1 Handle Purchase"", _team] call LogInform;" \n
       "" \n
       "_buyType = (_team) Call GetTeamType;" \n
       "if (_lastBuyType != _buyType) then {" \n
